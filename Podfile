source 'https://github.com/CocoaPods/Specs.git'
platform :ios, ‘9.0’

inhibit_all_warnings!

def mk_pods

pod 'Masonry', '~> 1.1.0'
pod 'Mantle', '2.1.1'
pod 'AFNetworking', '~> 3.0', :subspecs => ['Reachability', 'Serialization', 'Security', 'NSURLSession']


end

target 'demoProjects' do
mk_pods
end

